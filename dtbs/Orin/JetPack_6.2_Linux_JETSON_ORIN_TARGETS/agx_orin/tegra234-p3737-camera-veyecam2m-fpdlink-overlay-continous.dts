#include "tegra234-p3737-fpdlink-8chn-90ub954-common-continous.dtsi"
#include "tegra234-camera-fpdlink-veyecam2m-a00.dtsi"

/ {
	overlay-name = "Jetson Camera VEYE2M fpdlink module";
	jetson-header-name = "Jetson AGX CSI Connector";
	
	compatible = JETSON_COMPATIBLE;
	fragment@1 {
		target-path = "/";
		__overlay__ {
            /* all cameras are enabled by default */
             tegra-capture-vi {
                ports {
                     status = "okay";
                     port@0 {
                        status = "okay";
                         endpoint {
                            status = "okay";
                        };
                    };
                     port@1 {
                        status = "okay";
                         endpoint {
                            status = "okay";
                        };
                    };
                     port@2 {
                        status = "okay";
                         endpoint {
                            status = "okay";
                        };
                    };
                     port@3 {
                        status = "okay";
                         endpoint {
                            status = "okay";
                        };
                    };
                     port@4 {
                        status = "okay";
                         endpoint {
                            status = "okay";
                        };
                    };
                     port@5 {
                        status = "okay";
                         endpoint {
                            status = "okay";
                        };
                    };
                     port@6 {
                        status = "okay";
                         endpoint {
                            status = "okay";
                        };
                    };
                     port@7 {
                        status = "okay";
                         endpoint {
                            status = "okay";
                        };
                    };
                };
            };
            
            tegra-camera-platform {
                    compatible = "nvidia, tegra-camera-platform";
                    
                    /**
                    * tpg_max_iso = <>;
                    * Max iso bw for 6 streams of tpg
                    * streams * nvcsi_freq * PG_bitrate / RG10 * BPP
                    * 6 * 102Mhz * 32 bits/ 10 bits * 2 Bps
                    * = 3916.8 MBps
                    */
                    tpg_max_iso = <3916800>;

                    modules {
                         module0 {
                            status = "okay";
                             drivernode0 {
                                status = "okay";
                            };
                            
                        };
                         module1 {
                            status = "okay";
                             drivernode0 {
                                status = "okay";
                            };
                            
                        };
                         module2 {
                            status = "okay";
                             drivernode0 {
                                status = "okay";
                            };
                            
                        };
                         module3 {
                            status = "okay";
                             drivernode0 {
                                status = "okay";
                            };
                            
                        };
                         module4 {
                            status = "okay";
                             drivernode0 {
                                status = "okay";
                            };
                            
                        };
                        module5 {
                            status = "okay";
                             drivernode0 {
                                status = "okay";
                            };
                            
                        };
                        module6 {
                            status = "okay";
                             drivernode0 {
                                status = "okay";
                            };
                            
                        };
                        module7 {
                            status = "okay";
                             drivernode0 {
                                status = "okay";
                            };
                            
                        };
                    };
                };
                
        bus@0 {
            host1x@13e00000 {
                 nvcsi@15a00000 {
                    status = "okay";
                    num-tpg-channels = <36>;
                     channel@0 {
                        status = "okay";
                        ports {
				status = "okay";
                             port@0 {
                                status = "okay";
                                 endpoint@0 {
                                    status = "okay";
                                };
                            };
                             port@1 {
                                status = "okay";
                                 endpoint@1 {
                                    status = "okay";
                                };
                            };
                        };
                    };
                     channel@1 {
                        status = "okay";
                        ports {
				status = "okay";
                             port@0 {
                                status = "okay";
                                 endpoint@2 {
                                    status = "okay";
                                };
                            };
                             port@1 {
                                status = "okay";
                                 endpoint@3 {
                                    status = "okay";
                                };
                            };
