                                        endpoint {
                                            status = "okay";
                                        };
                                    };
                                };
                            };
                        };
                        i2c@3 {
                            reg = <3>;
                            i2c-mux,deselect-on-exit;
                            #address-cells = <1>;
                            #size-cells = <0>;
                             veyecam2m_fpd_g@3b {
                                status = "okay";
                                mode0 {
