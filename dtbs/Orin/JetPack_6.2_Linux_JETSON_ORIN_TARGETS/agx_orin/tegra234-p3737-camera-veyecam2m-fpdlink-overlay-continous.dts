                     channel@6 {
                        status = "okay";
                        ports {
				status = "okay";
                             port@0 {
                                status = "okay";
                                 endpoint@12 {
                                    status = "okay";
                                };
                            };
                             port@1 {
                                status = "okay";
                                 endpoint@13 {
                                    status = "okay";
                                };
                            };
                        };
                    };
                     channel@7 {
                        status = "okay";
                        ports {
				status = "okay";
                             port@0 {
                                status = "okay";
                                 endpoint@14 {
                                    status = "okay";
                                };
                            };
                             port@1 {
                                status = "okay";
                                 endpoint@15 {
                                    status = "okay";
                                };
                            };
                        };
                    };
                };
            };
            
                i2c@3180000 {
                    tca9548@70 {
                        status = "okay";
                        compatible = "nxp,pca9548";
                        reg = <0x70>;
                        #address-cells = <1>;
                        #size-cells = <0>;
                        skip_mux_detect;
                        i2c@0 {
                            reg = <0>;
                            i2c-mux,deselect-on-exit;
                            #address-cells = <1>;
                            #size-cells = <0>;
                             veyecam2m_fpd_a@3b {
                                status = "okay";
                                mode0 {
                                        discontinuous_clk = "no";
                                    };
                                ports {
                                    status = "okay";
                                    port@0 {
                                        status = "okay";
                                        endpoint {
                                            status = "okay";
                                        };
                                    };
                                };
                            };
                            veyecam2m_fpd_b@3c {
                                status = "okay";
                                mode0 {
                                        discontinuous_clk = "no";
                                    };
                                ports {
                                    status = "okay";
                                    port@0 {
                                        status = "okay";
                                        endpoint {
                                            status = "okay";
                                        };
                                    };
                                };
                            };
                        };
                        i2c@1 {
                            reg = <1>;
                            i2c-mux,deselect-on-exit;
                            #address-cells = <1>;
                            #size-cells = <0>;
                             veyecam2m_fpd_c@3b {
                                status = "okay";
                                mode0 {
                                        discontinuous_clk = "no";
                                    };
                                ports {
                                    status = "okay";
                                    port@0 {
                                        status = "okay";
                                        endpoint {
                                            status = "okay";
