/ {
	cam_i2cmux {
	i2c_0: i2c@0 {
        veye_vbyone0: vbyone_0@65 {
        compatible = "veye,vbyone";
        priority = <(-1)>;
        reg = <0x65>;
        status = "okay";
        csi-lane-count = <2>;
        coax-num = <1>;
        cam-i2c-pt-setting = <0x13>;
        camera-i2c-address=<0x3b>;
        
        trgin-gpio-mode = <0>;
        out1-gpio-mode = <0>;
        out2-gpio-mode = <0>;
        
        serializer {
            i2c-address=<0x34>;
            csi-lane-count = <2>;
            csi-lane-speed = <1500>;
            };
        };
    };
    i2c_1: i2c@1 {
    veye_vbyone1: vbyone_1@65 {
        compatible = "veye,vbyone";
        priority = <(-1)>;
        reg = <0x65>;
        status = "okay";
        csi-lane-count = <2>;
        coax-num = <1>;
        cam-i2c-pt-setting = <0x13>;
        camera-i2c-address=<0x3b>;
        
        trgin-gpio-mode = <0>;
        out1-gpio-mode = <0>;
        out2-gpio-mode = <0>;
        
        serializer {
            i2c-address=<0x34>;
            csi-lane-count = <2>;
            csi-lane-speed = <1500>;
            };
        };
    };
  };
};
