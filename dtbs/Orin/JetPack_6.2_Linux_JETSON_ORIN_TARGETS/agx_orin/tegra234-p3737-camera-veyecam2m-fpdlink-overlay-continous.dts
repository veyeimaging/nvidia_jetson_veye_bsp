                                    };
                                ports {
                                    status = "okay";
                                    port@0 {
                                        status = "okay";
                                        endpoint {
                                            status = "okay";
                                        };
                                    };
                                };
                            };
                        };
                        i2c@2 {
                            reg = <2>;
                            i2c-mux,deselect-on-exit;
                            #address-cells = <1>;
                            #size-cells = <0>;
                             veyecam2m_fpd_e@3b {
